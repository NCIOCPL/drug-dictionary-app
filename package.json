		"node": ">=18.0.0 <21.0.0",
		"npm": ">=9.0.0 <12.0.0"
	},
	"scripts": {
		"start": "node scripts/start.js",
		"start:silent": "BROWSER=none npm start",
		"debug": "node --inspect --inspect-brk ./node_modeles/webpack/bin/webpack.js",
		"dev:debug": "npm run debug -- --mode development",
		"build:cgdp-legacy": "node scripts/generate-cgdp-legacy.js",
		"build:cra": "node scripts/build.js",
		"build": "npm run build:cra && npm run build:cgdp-legacy",
		"test": "npm run unit-tests && npm run cy:ci && npm run merge-coverage",
		"test:it": "NODE_ENV=test cypress open",
		"test:pa11y": "./node_modules/.bin/pa11y-ci",
		"cy:ci": "start-server-and-test start:silent http-get://127.0.0.1:3000 integration-tests && nyc report -t coverage/cypress --reporter text --report-dir coverage/merged",
		"unit-tests": "jest",
		"integration-tests": "NODE_ENV=test cypress run && nyc report -t coverage/cypress --reporter text --report-dir coverage/cypress",
		"pa11y-tests": "start-server-and-test start http-get://127.0.0.1:3000 pa11y",
		"pa11y": "pa11y-ci",
		"merge-coverage": "npm run mc:pre && npm run mc:merge && npm run mc:report",
		"mc:pre": "node scripts/pre-merge-coverage",
		"mc:merge": "nyc merge coverage/tmp coverage/merged/coverage-final.json",
		"mc:report": "nyc report --nycrc-path=merged.nyc.config.js",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
		"lint": "NODE_ENV=test npm run lint:src && npm run lint:cypress",
		"lint:src": "NODE_ENV=test eslint src --ext .js,.jsx",
		"lint:cypress": "NODE_ENV=test eslint cypress/e2e --ext .js,.jsx",
		"lint:fix": "NODE_ENV=test npm run lint:src:fix && npm run lint:cypress:fix",
		"lint:src:fix": "NODE_ENV=test eslint src --ext .js,.jsx --fix",
		"lint:cypress:fix": "NODE_ENV=test eslint cypress/e2e --ext .js,.jsx --fix"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"ie 11"
		]
	},
	"babel": {
		"presets": [
			[
				"react-app",
				{
					"absoluteRuntime": false
				}
			]
		]
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": true,
		"stepDefinitions": "cypress/e2e",
		"commonPath": "cypress/e2e/common"
	},
	"devDependencies": {
		"@babel/core": "7.9.0",
		"@babel/eslint-parser": "^7.23.10",
